          },
          "query": "v = from(bucket: \"horus-bucket\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"sensor_readings\" and r.type == \"voltage\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> aggregateWindow(every: 1m, fn: mean, createEmpty: false)\n  |> rename(columns: {_value: \"voltage\"})\n\nc = from(bucket: \"horus-bucket\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"sensor_readings\" and r.type == \"current\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> aggregateWindow(every: 1m, fn: mean, createEmpty: false)\n  |> rename(columns: {_value: \"current\"})\n\njoin(tables: {v:v, c:c}, on: [\"_time\", \"site_id\"])\n  |> map(fn: (r) => ({r with _value: r.voltage * r.current, _measurement: \"power\"}))\n  |> keep(columns: [\"_time\", \"_value\", \"_measurement\", \"site_id\"])",
